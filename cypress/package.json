    "cypress-wait-until": "^1.7.2",
    "cypress-xpath": "^2.0.1",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-image-diff": "^1.25.0",
    "cypress-audit": "^1.4.0",
    "cypress-network-idle": "^1.0.0",
    "cypress-promise": "^1.0.2",
    "cypress-recurse": "^1.25.0",
    "cypress-iframe": "^1.0.1",
    "cypress-localstorage-commands": "^1.4.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.5.0",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-slack-reporter": "^1.3.0",
    "cypress-grep": "^3.1.0",
    "cypress-fail-fast": "^3.1.0",
    "cypress-skip-and-only-ui": "^1.0.0",
    "cypress-tags": "^1.0.0",
    "cypress-expect": "^2.5.0",
    "cypress-assert": "^1.0.0",
    "cypress-promise": "^1.0.2",
    "cypress-recurse": "^1.25.0",
    "cypress-iframe": "^1.0.1",
    "cypress-localstorage-commands": "^1.4.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.5.0",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-slack-reporter": "^1.3.0",
    "cypress-grep": "^3.1.0",
    "cypress-fail-fast": "^3.1.0",
    "cypress-skip-and-only-ui": "^1.0.0",
    "cypress-tags": "^1.0.0",
    "cypress-expect": "^2.5.0",
    "cypress-assert": "^1.0.0"
  },
