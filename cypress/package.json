{
  "name": "wordweave-cypress-tests",
  "version": "1.0.0",
  "description": "Cypress E2E tests for WordWeave",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headless": "cypress run --headless",
    "cy:run:record": "cypress run --record",
    "cy:run:parallel": "cypress run --parallel",
    "cy:run:ci": "cypress run --ci",
    "cy:component": "cypress run --component",
    "cy:component:open": "cypress open --component",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "cypress run --ci --browser chrome",
    "test:e2e:parallel": "cypress run --parallel --record",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:visual": "cypress run --env percy=true",
    "test:accessibility": "cypress run --env a11y=true",
    "test:performance": "cypress run --env performance=true",
    "test:mobile": "cypress run --env mobile=true",
    "test:tablet": "cypress run --env tablet=true",
    "test:desktop": "cypress run --env desktop=true",
    "test:responsive": "cypress run --env responsive=true",
    "test:themes": "cypress run --env themes=true",
    "test:animations": "cypress run --env animations=true",
    "test:errors": "cypress run --env errors=true",
    "test:api": "cypress run --env api=true",
    "test:full": "cypress run --env full=true",
    "lint": "eslint cypress/**/*.{js,ts} --ext .js,.ts",
    "lint:fix": "eslint cypress/**/*.{js,ts} --ext .js,.ts --fix",
    "format": "prettier --write cypress/**/*.{js,ts}",
    "type-check": "tsc --noEmit",
    "clean": "rimraf cypress/videos cypress/screenshots cypress/downloads",
    "clean:all": "rimraf cypress/videos cypress/screenshots cypress/downloads node_modules .cypress"
  },
  "dependencies": {
    "cypress": "^13.6.0",
    "cypress-axe": "^1.4.0",
